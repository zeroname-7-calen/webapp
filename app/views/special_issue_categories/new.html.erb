<div class="sp_iss_cate_new_wrapper">
  <div class="sp_iss_cate_new_title">
    <% @page_title = "コラムシリーズを新規作成する" %>
    <h5><%= @page_title %></h5>
  </div>
  <div class="sp_iss_cate_new_form">
    <%= form_for @special_issue_category do |form| %>
      <%= render "form", form: form %>
      <div class="sp_iss_cate_create_button">
        <%= form.submit class: "btn btn-primary btn-md"%>
      </div>
    <% end %>
  </div>
</div>
